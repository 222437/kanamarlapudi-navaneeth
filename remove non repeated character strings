#include<stdio.h>
#include<string.h>
int main()
{
        char str[100],result[100];
        int i,j,count,pos=0;

          printf("Enter a string(no spaces):");
          scanf("%s",str);
          int len=strlen(str);
          for(i=0;i<len;i++)
          {
                  count=0;


          for(j=0;j<len;j++)
          {
                if(str[i]==str[j])
                count++;
          }
          if(count>1)
          {
                result[pos++]=str[i];
          }
     }
     result[pos]='\0';
     printf("string after removing non-repeated charcters:%s\n",result);
     return 0;
}
                                                      
